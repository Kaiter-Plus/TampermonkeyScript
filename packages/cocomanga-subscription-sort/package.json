{
  "name": "cocomanga-subscription-sort",
  "version": "0.8.0",
  "type": "module",
  "author": "Kaiter-Plus",
  "description": "COCO 漫画“我的收藏”界面根据“更新日”进行排序",
  "repository": {
    "url": "https://gitee.com/Kaiter-Plus/TampermonkeyScript/tree/master/packages/cocomanga-subscription-sort"
  },
  "license": "BSD-3-Clause",
  "buildOptions": {
    "name": {
      "default": "COCO漫画收藏按更新时间排序"
    },
    "spaces": 12,
    "payload": {
      "icon": [
        "https://www.colamanga.com/favicon.ico"
      ],
      "match": [
        "*://*.cocomanhua.com/dynamic/user/subscription",
        "*://*.cocomanga.com/dynamic/user/subscription",
        "*://*.colamanhua.com/dynamic/user/subscription",
        "*://*.colamanga.com/dynamic/user/subscription"
      ],
      "run-at": [
        "document-end"
      ],
      "grant": [
        "GM_addStyle"
      ]
    }
  }
}